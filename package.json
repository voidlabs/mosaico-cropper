  "keywords": [
    "jquery",
    "ui",
    "widget",
    "crop",
    "resize",
    "image",
    "mosaico"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:voidlabs/mosaico-cropper.git"
  },
  "author": "Stefano Bagnara",
  "license": "MIT",
  "licenseurl": "http://www.opensource.org/licenses/mit-license.php",
  "homepage": "https://github.com/voidlabs/mosaico-cropper",
  "bugs": "https://github.com/voidlabs/mosaico-cropper/issues",
  "peerDependencies": {
    "jquery": "^3.6.0",
    "jquery-ui-package": "^1.11.4",
    "jquery-ui-touch-punch": "^0.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "10.4.21",
    "cssnano": "^7.1.1",
    "eslint": "^9.34.0",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "jimp": "^1.6.0",
    "jsdom": "^26.1.0",
    "less": "^4.4.1",
    "postcss": "^8.5.6",
    "terser": "^5.43.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "async": "3.2.3",
    "minimist": "1.2.6",
    "shelljs": "0.8.5",
    "superagent": "7.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 1%",
    "IE 10",
    "IE 11"
  ],
  "type": "module",
  "scripts": {
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "dev": "vite",
    "build": "npm run lint && vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "lockdeps": "npx npm-force-resolutions",
